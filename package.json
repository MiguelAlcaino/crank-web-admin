{
  "name": "crank-web-admin",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/crank-web-admin.umd.cjs",
  "module": "./dist/crank-web-admin.js",
  "exports": {
    ".": "./dist/crank-web-admin.js",
    "./dist/style.css": "./dist/style.css"
  },
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "codegen": "graphql-codegen --config codegen.ts",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@imengyu/vue3-context-menu": "^1.3.3",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "@vueform/slider": "^2.1.9",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vuepic/vue-datepicker": "^6.1.0",
    "axios": "^1.5.1",
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.10.3",
    "dayjs": "^1.11.10",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.33",
    "react": "^18.2.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue-tel-input": "^8.1.4",
    "vue3-popper": "^1.5.0",
    "vue3-simple-typeahead": "^1.0.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/client-preset": "^1.3.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.18.58",
    "@types/vue-tel-input": "^2.1.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.7.0",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.5",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7"
  }
}
